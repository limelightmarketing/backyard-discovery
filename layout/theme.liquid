      href="https://www.backyarddiscovery.com{{ request.path }}"
      hreflang="x-default" />
    <link
      rel="alternate"
      href="https://www.backyarddiscovery.com{{ request.path }}"
      hreflang="en-us" />
    <link
      rel="alternate"
      href="https://www.backyarddiscovery.com{{ request.path }}"
      hreflang="en" />

    {% if template.name == 'product' %}
      {% assign = hreflang_markup = product.metafields.custom.hreflang %}
    {% elsif template.name == 'page' %}
      {% assign = hreflang_markup = page.metafields.custom.hreflang %}
    {% elsif template.name == 'collection' %}
      {% assign = hreflang_markup = collection.metafields.custom.hreflang %}
    {% elsif template.name == 'index' %}
      {% assign = hreflang_markup = "1" %}
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.co.uk{{ request.path }}"
        hreflang="en-gb" />
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.ca{{ request.path }}"
        hreflang="en-ca" />
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.ca/fr{{ request.path }}"
        hreflang="fr" />
    {% endif %}

    {% if hreflang_markup == 1 %}
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.co.uk{{ request.path }}"
        hreflang="en-gb" />
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.ca{{ request.path }}"
        hreflang="en-ca" />
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.ca/fr{{ request.path }}"
        hreflang="fr" />
    {% endif %}

    {% if hreflang_markup == 2 %}
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.ca{{ request.path }}"
        hreflang="en-ca" />
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.ca/fr{{ request.path }}"
        hreflang="fr" />
    {% endif %}

    {% if hreflang_markup == 3 %}
      <link
        rel="alternate"
        href="https://www.backyarddiscovery.co.uk{{ request.path }}"
        hreflang="en-gb" />
    {% endif %}

