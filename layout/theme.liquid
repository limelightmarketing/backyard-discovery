.eci-question p {
  font-weight: 700;
  display: inline-block;
  margin-left: 20px;
  vertical-align: top;
  width: calc(100% - 40px);
}
.eci-question span.toogle-icon{
  display: inline-block;
  vertical-align: top;
}
.eci-question span.toogle-icon:before {
      content: "+";
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      color: #2b292d;
      transition: transform .25s ease-out;
      font-family:Montserrat, sans-serif;
      font-weight: 600;
      font-size: 25px;
      line-height: 21px;
      display: inline-block;
      vertical-align: top;
      width: 15px;
}
.eci-question.eci-active span.toogle-icon:before {
  content: "-";
}
.eci-answer {
  padding: 0 20px 20px 25px;
}
  </style>
  <script>
  jQuery(document).on('click', '.eci-question', function(){
    var eci_parent = jQuery(this).closest('.eci-faq-question');
    var eci_this_answer = eci_parent.find('.eci-answer');
    var eci_this_question = eci_parent.find('.eci-question');
    eci_this_answer.toggle();
    eci_this_question.toggleClass('eci-active');
    jQuery('.eci-faq-question').each(function(){
        if(jQuery(this).index() != eci_parent.index()){
            jQuery(this).find('.eci-answer').hide();
            jQuery(this).find('.eci-question').removeClass('eci-active');
        }
    })      
  });
  </script>
{% endif %}

  
