    const $openModalButton = $('#open-modal');
    const $closeModalButton = $('#close-modal');
    const $modalOverlay = $('#modal-overlay');
    // Hide the modal initially
    $openModalButton.each(function() {
      $modalOverlay.hide();
      
      // When the open modal button is clicked, show the modal and disable page scrolling
      $(this).on('click', function() {
        $modalOverlay.show();
        $('body').css('overflow', 'hidden');
        $('body').addClass('overflow', 'hidden');
      });
  
      // When the close modal button is clicked, hide the modal and enable page scrolling
      $closeModalButton.on('click', function() {
        $modalOverlay.hide();
        $('body').css('overflow', 'auto');
      });
  
      // When the user clicks outside the modal or on the overlay, hide the modal and enable page scrolling
      $modalOverlay.on('click', function(event) {
        if (event.target.id === 'modal-overlay') {
          $modalOverlay.hide();
          $('body').css('overflow', 'auto');
        }
      });
    });
  });

$(document).ready(function(){
