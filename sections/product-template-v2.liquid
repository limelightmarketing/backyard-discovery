{% render 'loading-spinner' %}
   <link
    rel="stylesheet"
      href="{{ 'custom.css' | asset_url }}"
      type="text/css">
<style>
  .grid {
    display: inherit;
}
  .product-template__variation-radio-item div[data-option='Shipping Included'] {
    display: none;
}

</style>

{% if product.tags contains '360' or 'v360' %}
<script>
  	(function(s,i,r,v){
      v = i.createElement(r); v.async=0;
      v.src= 'https://scripts.sirv.com/sirv.js';
      i.getElementsByTagName(r)[0].parentNode.appendChild(v);
	})(window, document, 'script');
</script>
    <style>
      .MTGallery  {
        display: none;
      }
      .MagicZoomPlus  {
        display: none!important;
      }
      #SirvIcon {
        display: none;
      }
    </style>
{% endif %}


<div class="product-v2 product-template__container page-width"
  id="ProductSection-{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="product"
  data-enable-history-state="true"
  data-ajax-enabled="{{ settings.enable_ajax }}"
>
<div class="product-template__breadcrumbs-wrapper">
  {% render 'breadcrumbs' %}
</div>
  
  {% comment %}
    Get first variant, or deep linked one
  {% endcomment %}
  {% liquid assign current_variant = product.selected_or_first_available_variant
    assign product_image_zoom_size = '1024x1024'
    assign product_image_scale = '2'
    assign enable_image_zoom = section.settings.enable_image_zoom
    assign compare_at_price = current_variant.compare_at_price
    assign price = current_variant.price
    case section.settings.media_size
      when 'small'
        assign product_media_width = 'medium-up--one-third'
        assign product_description_width = 'medium-up--two-thirds'
        assign height = 345
      when 'medium'
        assign product_media_width = 'medium-up--one-half'
        assign product_description_width = 'medium-up--one-half'
        assign height = 530
      when 'large'
        assign product_media_width = 'medium-up--two-thirds'
        assign product_description_width = 'medium-up--one-third'
        assign height = 720
      when 'full'
        assign product_media_width = ''
        assign product_description_width = ''
        assign height = 1090
        assign enable_image_zoom = false
    endcase %}
  
  <div class="p_info mobile">
    <h1 class="product-single__title">{{ product.title }}</h1>
  
    <script>window.performance.mark('debut:product:title_visible');</script>

    <div style="display:flex;">
      <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>
      <a href="#form_{{ product.id }}" class="open-write-review" style="text-decoration:underline;padding-left:15px;">Write a Review</a>
  </div>

  <script>
    document.getElementsByClassName('open-write-review')[0].addEventListener('click', function () {
      document.getElementsByClassName('spr-form')[0].style.display='block';;
    });
  </script>

  <div class="product__price affirm-price-container">
    {% render 'product-price', variant: current_variant, show_vendor: section.settings.show_vendor %}
  </div>

  {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
    <div class="product__policies rte" data-product-policies>
      {%- if shop.taxes_included -%}
        {{ 'products.product.include_taxes' | t }}
      {%- endif -%}
      {%- if shop.shipping_policy.body != blank -%}
        {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
      {%- endif -%}
    </div>
  {%- endif -%}

  <div class="shoppay"></div>

  <div class="shipping-included__wrapper">
    <span class="shipping-included__text">{{ 'products.product.shipping_included' | t }} | </span>
    <a id="OpenArrival" href="#" class="shipping-included__link"
      onClick="ga('send', 'event', 'PDP Arrival Date Click', 'Click', '{{ product.title | escape }}');"> {{ 'products.product.check_arrival_dates' | t }}</a>
  </div>
</div>
  
  <div class="grid product-single{% if section.settings.enable_payment_button %} product-single--{{ section.settings.media_size }}-media{% endif %}">
    <div class="timesact-badge grid__item product-single__media-group {{ product_media_width }}{% if section.settings.media_size == 'full' %} product-single__media-group--full{% endif %}" data-product-single-media-group>

      {%- assign featured_media = product.selected_or_first_available_variant.featured_media | default: product.featured_media -%}

      {% for media in product.media %}
        {% if media.media_type == 'model' %}
          <div class="view--in-your-space medium-up--four-fifths mobile small---hide">
            <a href="{{ shop.url }}{{ product.url }}?arqrcode=1&utm_source={{ product.title | escape }}&utm_medium=View%20QR%20Code&utm_campaign=AR%20Experience">
              <span class="btn button open_qr_pop">SEE IT IN YOUR PLACE</span>
            </a>               
          </div>
        {% endif %}
      {% endfor %}  
  
      <div class="pswp--gallery slider-for">
        {% assign pswpCount = 1 %}        
        {% for media in product.media %}
          <div class="thumbnails-wrapper pswp-gallery product-single__thumbnails-item product-single__thumbnails-item--{{ section.settings.media_size }} {% if enable_thumbnail_slides == true %} product-single__thumbnails-item-slide{% endif %} {%- if media.media_type == 'video' or media.media_type == 'external_video' -%} product-single__thumbnails-item--video{% endif %} js"{% if enable_thumbnail_slides == true %} {% if video_code != blank %}data-slider-slide-index="{{ forloop.index1 }}"{% else %}data-slider-slide-index="{{ forloop.index0 }}"{% endif %} data-slider-item{% endif %} {% if media.media_type == 'model' %}data-product-media-type-model{% endif %}>

            {% comment %}
              .pswp-item
            {% endcomment %}
            {% if media.media_type == 'image' %}
              <figure class="pswp-item">
            {% else %}
              <div>
            {% endif %}

                <a href="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"
                  class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }}"
                  data-thumbnail-id="{{ section.id }}-{{ media.id }}"
                  {% if media.media_type == 'image' %}data-index="{{ pswpCount }}" data-size="1000x1000"{% endif %}
                  {% if enable_thumbnail_slides == true %} data-slider-item-link{% endif %}
                  {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>

                    {%- capture thumbnailAlt -%}
                      {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
                        {{ 'sections.featured_product.video_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- elsif media.media_type == 'model' -%}
                        {{ 'sections.featured_product.model_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- else -%}
                        {{ 'sections.featured_product.gallery_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                      {%- endif -%}
                    {%- endcapture -%}

                    {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
                      <div class="product-single__thumbnail--video">
                        <div class="product-single__thumbnail--video--inner">
                          <i class="fas fa-video"></i>
                          <span class="product-single__thumbnail-label--video">Video</span>
                        </div>
                      </div>
                    {% else %}
                      {%- if media.media_type != 'model' -%}
                        <img class="product-single__thumbnail-image" src="{{ media.preview_image | img_url: 'master', scale: 2 }}" alt="{{ thumbnailAlt }}">
                      {% endif %}
                    {% endif %}

                    {%- if media.media_type == 'model' -%}
                      <div class="product-single__media" style="padding-top: 100%">
                        {{ media | model_viewer_tag: image_size: image_size, reveal: 'interaction', toggleable: true, data-model-id: media.id }}
                      </div>
                    {%- endif -%}
                </a>

                {% comment %}
                  /.pswp-item
                {% endcomment %}
                {% if media.media_type == 'image' %}
                  </figure>
                  {% assign pswpCount = pswpCount | plus: 1 %}
                {% else %}
                  </div>
                {% endif %}
          </div>
        {% endfor %}

        {% if product.metafields.custom.gallery_video %}
          <div class="product-single__thumbnails-item product-single__thumbnails-item--youtube product-single__thumbnails-item--medium  product-single__thumbnails-item-slide product-single__thumbnails-item--video js" data-slider-slide-index="0" data-slider-item>
            <div class="text-link play-video-modal" style="cursor: pointer;" onClick="ga('send', 'event', 'PDP Watched Video', 'Click', '{{product.title | escape}}');">
              <div class="product-single__thumbnail--video">
                <div class="product-single__thumbnail--video--inner" style="background-size: cover; background-position: center; background-image: url({{ product.media[0].preview_image | img_url: 'master', scale: 2 }})">
                  <div style="position: absolute; top: 0; left: 0; width: 100%; height: 100%; background-color: rgba(0, 0, 0, 0.5);"></div>
                  <i class="fa fa-play text-white fa-5x" style="position: relative"></i>
                </div>
              </div>
            </div>
          </div>
        {% endif %}
      </div>


      <noscript>
        {% capture product_image_size %}{{ height }}x{% endcapture %}
        <img src="{{ featured_media | img_url: product_image_size, scale: product_image_scale }}" alt="{{ featured_media.alt }}" id="FeaturedMedia-{{ section.id }}" class="product-featured-media" style="max-width: {{ height }}px;">
      </noscript>

      {% assign first_3d_model = product.media | where: "media_type", "model" | first %}

      {%- if first_3d_model -%}
        <button
          aria-label="{{ 'products.product.view_in_space_label' | t }}"
          class="product-single__view-in-space"
          data-shopify-xr
          data-shopify-model3d-id="{{ first_3d_model.id }}"
          data-shopify-title="{{ product.title | escape }}"
          data-shopify-xr-hidden
          onClick="ga('send', 'event', 'AR Experience', 'View In Space', '{{ product.title | escape }}');"
        >
          {% render 'icon-3d-badge-full-color' %}<span class='product-single__view-in-space-text'>{{ 'products.product.view_in_space' | t }}</span>
        </button>
      {%- endif -%}


      {% if product.media.size > 1 %}
        {% if product.media.size > 4 %}
          {%- assign enable_thumbnail_slides = false -%}
        {% endif %}

        <div data-thumbnail--slider>
          <div class="thumbnails-wrapper{% if enable_thumbnail_slides == true %} slider-active{% endif %}" data-slider>
            {% if enable_thumbnail_slides == true %}
              <button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__prev thumbnails-slider__prev--{{ section.id }}" data-slider-button>
                {% render 'icon-chevron-left' %}
                <span class="icon__fallback-text">{{ 'sections.slideshow.previous_slide' | t }}</span>
              </button>
            {% endif %}

            <ul class="slider-nav product-single__thumbnails product-single__thumbnails-{{ section.id }}" data-slider-container>
              {% for media in product.media %}
                <li class="product-single__thumbnails-item product-single__thumbnails-item--{{ section.settings.media_size }} {% if enable_thumbnail_slides == true %} product-single__thumbnails-item-slide{% endif %} {%- if media.media_type == 'video' or media.media_type == 'external_video' -%} product-single__thumbnails-item--video{% endif %} js"{% if enable_thumbnail_slides == true %} {% if product.metafields.custom.gallery_video != '' %}data-slider-slide-index="{{ forloop.index1 }}"{% else %}data-slider-slide-index="{{ forloop.index0 }}"{% endif %} data-slider-item{% endif %}>
                    <div>
                      <a 
                        class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }}"
                        {% if enable_thumbnail_slides == true %} data-slider-item-link{% endif %}
                        {% if enable_image_zoom %}data-zoom="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>

                          {%- capture thumbnailAlt -%}
                            {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
                              {{ 'sections.featured_product.video_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                            {%- elsif media.media_type == 'model' -%}
                              {{ 'sections.featured_product.model_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                            {%- else -%}
                              {{ 'sections.featured_product.gallery_thumbnail_alt' | t: imageAlt: media.alt | escape }}
                            {%- endif -%}
                          {%- endcapture -%}

                          {%- if media.media_type == 'video' or media.media_type == 'external_video' -%}
                            <div class="product-single__thumbnail--video">
                              <div class="product-single__thumbnail--video--inner">
                                <i class="fas fa-video"></i>
                                <span class="product-single__thumbnail-label--video">Video</span>
                              </div>
                            </div>
                          {% else %}
                            <img class="product-single__thumbnail-image" src="{{ media.preview_image | img_url: '110x110', scale: 2 }}" alt="{{ thumbnailAlt }}">
                          {% endif %}

                          {%- if media.media_type == 'model' -%}
                            <div class="product-single__thumbnail-badge">
                              {% render 'icon-3d-badge-full-color' %}
                            </div>
                          {%- endif -%}
                      </a>
                    </div>
                </li>
              {% endfor %}

              {% if product.metafields.custom.gallery_video != blank %}
                <li class="product-single__thumbnails-item product-single__thumbnails-item--youtube product-single__thumbnails-item--medium  product-single__thumbnails-item-slide product-single__thumbnails-item--video js" data-slider-slide-index="0" data-slider-item>
                  <div class="text-link" onClick="ga('send', 'event', 'PDP Watched Video', 'Click', '{{product.title | escape}}');">
                    <div class="product-single__thumbnail--video">
                      <div class="product-single__thumbnail--video--inner" style="background-size: cover; background-position: center; background-image: url({{ product.media[0].preview_image | img_url: 'master', scale: 2 }})">
                        <div style="position: absolute; top: 0; left: 0; width: 100%; height: 100%; background-color: rgba(0, 0, 0, 0.5);"></div>
                        <i class="fa fa-play text-white" style="position: relative"></i>
                      </div>
                    </div>
                  </div>
                </li>
              {% endif %}
              
            </ul>
        
            {% if enable_thumbnail_slides == true %}
              <button type="button" class="btn btn--link medium-up--hide thumbnails-slider__btn thumbnails-slider__next thumbnails-slider__next--{{ section.id }}" data-slider-button data-slider-button-next>
                {% render 'icon-chevron-right' %}
                <span class="icon__fallback-text">{{ 'sections.slideshow.next_slide' | t }}</span>
              </button>
            {% endif %}
          </div>
        </div>
      {% endif %}

      {% if section.settings.show_share_buttons %}
          {% render 'social-sharing', share_title: product.title, share_permalink: product.url, share_image: product.featured_media %}
        {% endif %}

    <div class="trust_img desktop"> 

      <div class="md:flex w-full justify-center items-center px-12 md:px-4 md:py-8 py-2 hidden">
            {% if section.settings.include_protect_certification_icon %}<img loading="lazy" width="100" height="100" class="protect lg:w-3/12 w-4/12 mx-4" src="https://cdn.shopify.com/s/files/1/0545/0041/files/Pro-Tect-burnt-orange.png?v=1679676271">{% endif %}
            {% if section.settings.include_5year_icon %}<img loading="lazy" width="100" height="100" class="warrenty lg:w-3/12 w-4/12 mx-4" src="https://cdn.shopify.com/s/files/1/0545/0041/files/5-year-warrenty.png?v=1679676271">{% endif %}
            {% if section.settings.include_guided_assembly_icon %}<img loading="lazy" width="100" height="100" class="assembly lg:w-3/12 w-4/12 mx-4" src="https://cdn.shopify.com/s/files/1/0545/0041/files/guided-assembly.png?v=1679676271">{% endif %}
          </div>

    </div>

      
    </div>

    
<div class="grid__item {{ product_description_width }}">
<div class="product-single__meta">


  <div class="product-info-desktop desktop">
<h1 class="product-single__title">{{ product.title }}</h1>


  <script>window.performance.mark('debut:product:title_visible');</script>

          <div style="display:flex;">
          <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>
          <a href="#form_{{ product.id }}" class="open-write-review" style="text-decoration:underline;padding-left:15px;">Write a Review</a>
        </div>

    <div class="flex">
      {%- if product.available == false -%}
        <div class="uppercase text-xs font-medium pr-4">Out of stock</div> 
      {%- endif -%}
      {%- if product.tags contains 'new arrival' -%}
        <div class="uppercase text-xs font-medium pr-4">New Arrival</div> 
      {%- endif -%}
      {%- if product.tags contains 'Best Seller' or product.tag contains 'Top Seller' -%}
        <div class="uppercase text-xs font-medium">Best Seller</div> 
      {%- endif -%}
    </div>

        <script>
          window.performance.mark('debut:product:title_visible');
        </script>
        {% unless product.tags contains 'coming soon' or product.tags contains 'pre-order' %}
          <div id="BVRRSummaryContainer" class="product-single__inline-ratings"></div>
        {%- endunless -%}

        {%- if product.tags contains 'coming soon' -%}
          <div>
            <h3>Coming Soon!</h3>
          </div>
        {%- endif -%}
        {% unless product.tags contains 'coming soon' %}
          <div class="product__price affirm-price-container">
            {% render 'product-price', variant: current_variant, show_vendor: section.settings.show_vendor %}
          </div>

          {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
            <div class="product__policies rte" data-product-policies>
              {%- if shop.taxes_included -%}
                {{ 'products.product.include_taxes' | t }}
              {%- endif -%}
              {%- if shop.shipping_policy.body != blank -%}
                {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
              {%- endif -%}
            </div>
          {%- endif -%}

      
        {%- endunless -%}

  </div>
    

        {% capture "form_classes" -%}
          product-form product-form-{{ section.id }}
          {%- if section.settings.enable_payment_button and product.has_only_default_variant %}
            product-form--payment-button-no-variants
          {%- endif -%}
          {%- if product.available == false %}
            product-form--variant-sold-out
          {%- endif -%}
        {%- endcapture %}

        {% form 'product', product, class: form_classes, novalidate: 'novalidate', data-product-form: '' %}
          {% comment %}
          Automatically inserted by Shop Pay Installments theme updater
          {% endcomment %}
          
          {% unless settings.disable_autogenerated_payment_terms %}
            <div class="desktop">
            {{ form | payment_terms }}
            </div>
            
          {% endunless %}


          <a href="javascript:void()" id="show_ship" style="font-size:0px;padding:0;display:inherit;">...</a>
         <div class="shipping-included__wrapper desktop">
            <span class="shipping-included__text">{{ 'products.product.shipping_included' | t }} |
            </span>
            <a
              id="OpenArrival"
              href="#"
              class="shipping-included__link"
              onClick="ga('send', 'event', 'PDP Arrival Date Click', 'Click', '{{product.title | escape}}');">
              {{ 'products.product.check_arrival_dates' | t }}</a>
          </div>
                
          {% unless product.has_only_default_variant %}
            <div class="product-template__variation-radios">
              {% for option in product.options_with_values %}
                {% unless option.name contains "Shipping" %}
                  <div class="product-template__variation-radio-item">
                    {% liquid for variant in product.variants
                      for value in option.values
                        unless value == 'Shipping & Assembly Included'
                          assign assembly = variant.price
                        endunless
                      endfor
                    endfor %}
                    {% for value in option.values %}
                      {% unless value == "Shipping Included" %}
                        <div data-option="{{ value | escape }}" data-price="{{ assembly | money_without_trailing_zeros }}" class="product-template__variation-radio-option">
                          <div class="variation-radio__circle"></div>
                          <div class="variation-radio__label">
                            {{ 'products.product.add_assembly' | t }} (+{{ assembly | minus: price | money_without_trailing_zeros }})
                          </div>
                        </div>
                      {% endunless %}
                    {% endfor %}
                  </div>
                  {% endunless %}
              {% endfor %}
            </div>

            {% comment %}
            Hidden variations form. 
            Custom radio buttons in above component controls the form and adjusts the price.
            {% endcomment %}
            <div class="product-form__controls-group">
              {% for option in product.options_with_values %}
                {% unless option.name contains 'Shipping' %}
                  <div class="selector-wrapper js product-form__item">
                    <label for="SingleOptionSelector-{{ forloop.index0 }}">
                        {{ option.name }}
                    </label>
                    <select
                      class="single-option-selector single-option-selector-{{ section.id }} product-form__input"
                      id="SingleOptionSelector-{{ forloop.index0 }}"
                      data-index="option{{ forloop.index }}">
                      {% for value in option.values %}
                        <option value="{{ value | escape }}"{% if option.selected_value == value %}selected="selected"{% endif %}>{{ value }}</option>
                      {% endfor %}
                    </select>
                  </div>
                {% endunless %}
              {% endfor %}
            </div>
          {% endunless %}

          <select
            name="id"
            id="ProductSelect-{{ section.id }}"
            class="product-form__variants no-js">
            {% for variant in product.variants %}
              {% unless variant.title contains 'Assembly' %}
                <option
                  value="{{ variant.id }}"
                  {%- if variant == current_variant %}
                  selected="selected"
                  {%- endif -%}>
                  {{ variant.title }}
                  {%- if variant.available == false %}
                    - {{ 'products.product.sold_out' | t }}{% endif %}
                </option>
              {% endunless %}
            {% endfor %}
          </select>

          {% if section.settings.show_quantity_selector %}
            <div class="product-form__controls-group">
              <div class="product-form__item">
                <label for="Quantity-{{ section.id }}">{{ 'products.product.quantity' | t }}</label>
                <input
                  type="number"
                  id="Quantity-{{ section.id }}"
                  name="quantity"
                  value="1"
                  min="1"
                  pattern="[0-9]*"
                  class="product-form__input product-form__input--quantity"
                  data-quantity-input>
              </div>
            </div>
          {% endif %}

          <div class="available-sizes pt-4 md:pt-0">{{ 'products.product.available_sizes' | t }}</div>
          <div class="filters-toolbar__item-child filters-toolbar__item-child--selectable mb-6">
            <div class="filters-toolbar__input-wrapper select-group ">
              <select name="size" id="size"
                class="filters-toolbar__input hidden "
                aria-describedby="a11y-refresh-page-message a11y-selection-message"
              >
                <option value="">{{ product.title }}</option>
                {% for product_size_variant in product.metafields.custom.size_variations.value %}
                  <option value="{{ product_size_variant.url }}">
                    {{ product_size_variant.title }}
                  </option>
                {% endfor %}
              </select>
              {% render 'icon-chevron-down' %}
            </div>
          </div>

          <div class="add-to-cart--mobile-only">
            {% render 'add-to-cart' %}
          </div>

         <div class="custom_tabs desktop">  
          <div class="description">{{ product.description }}</div>

          <div
            class="product-form__error-message-wrapper product-form__error-message-wrapper--hidden{% if section.settings.enable_payment_button %} product-form__error-message-wrapper--has-payment-button{% endif %}"
            data-error-message-wrapper
            role="alert">
            <span class="visually-hidden">{{ 'general.accessibility.error' | t }}</span>
            {% render 'icon-error' %}
            <span class="product-form__error-message" data-error-message>{{ 'products.product.quantity_minimum_message' | t }}</span>
          </div>

          {% render 'add-to-cart' %}

          <div class="accordion">
              {% if product.metafields.custom.details %}
                <div class="accordion-header">{{ 'products.product.details' | t }} <span class="accordion-icon">+</span></div>
                <div class="accordion-content">
                  {{ product.metafields.custom.details | metafield_tag }}
                  <br />
                  <div class="variant-sku">Item #<span id="sku">{{ current_variant.sku }}</span></div>
                </div>
              {% endif %}
