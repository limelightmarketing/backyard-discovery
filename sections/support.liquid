{% stylesheet %}


.playing-overlay{
  content: 'Now Playing';
  box-shadow: inset 0 0 0 1000px rgba(0,0,0,.35);
}
.playing-text{
  visibility: hidden;
}

.pr{
	padding-right: 10px;
}
.pl{
	padding-left: 10px;
}

.pr:hover{
  color: var(--color-primary) !important;
}
.pl:hover{
  color: var(--color-primary) !important;
}

@media (max-width: 780px) {
.pr{
	padding: 0;
}
.pl{
	padding: 0;
}
}



.w-33{
	width: 33%;
}


@media (max-width: 780px) {
.w-33{
	width:100%;
}
.dir-column{
flex-direction: column;
}
}
  
.w-75{
  width: 75%;
  }
  
.w-25 {
  width: 25%;
  }
  
@media (max-width: 942px) {
.w-75{
  width: 100%;
  }
  
.w-25 {
  width: 100%;
  }
}


  
 
{% endstylesheet %}

