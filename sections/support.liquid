<div style="">
    {%- include 'section-header' -%}    

<div class="feature-row-wrapper-templates">
  <div class="page-width flex-wrap-sm" style="  display:flex;  margin: 0 auto; justify-content: flex-end; padding: 20px 55px">
    <div class="w-25" style="padding: 1rem; ">
        
         {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'link_list' -%}
                {%- assign footer_linklist = block.settings.menu -%}

                  <ul class="site-side-nav__linklist
                            {% if inline_list %}list--inline{% endif %}">
                    {%- for link in linklists[footer_linklist].links -%}
                      <li class="site-side-nav__linklist-item" style=" font-weight:600; padding-bottom: 10px;">
                        <a href="{{ link.url }}"
                          {% if link.current %} aria-current="page"{% endif %}
                        >
                          {{ link.title | escape }}
                        </a>
                      </li>
                    {%- endfor -%}
                  </ul>
            {%- endcase -%}
          {%- endfor -%}
    </div>
    
    
    <div class="w-75">
    
    
	{%- for block in section.blocks -%}
      {%- case block.type -%}
      
<!--      heading  -->

{%- when 'textarea' -%}    
{%- include 'h-tags.liquid' -%}

<!--     end heading  -->


<!--      Content  -->
      
        {%- when 'richtext' -%}    

     		 <p> {{ block.settings.page_content }}</p>
           


         {%- when 'page_content' -%}    

     		 <p>{{ page.content }} </p>


<!--     End Content  -->
 
 
<!--      Button  -->
      
{%- when 'button' -%}    
{% include 'button' %}


<!--      End Button  -->
 
<!--      Image Picker  -->

{%- when 'image_layout' -%}
{% include 'image-picker' %}


          
<!--      End Image Picker  -->

          
<!--      Cards  -->
      
{%- when 'cards' -%}

{% include 'cards' %}


<!--      End Cards  -->

<!--      Full Width Image  -->

{%- when 'full_image' -%}
  <div style="background-image: url('{{ block.settings.image_picker  | img_url: '2048x' }}'); background-position: center; background-repeat: no-repeat; width:100%; height: 500px; ">
  
  </div>


<!--      End Full Width Image  -->

{%- when 'padding' -%}
      
<div style="padding:{{ block.settings.padding_size }}px;"></div>

<!--      Video  -->

{%- when 'video' -%}
{%- include 'full-video' -%}


<!--      End Video  -->


<!--      Video Section -->

{%- when 'video_featured_selection' -%}


<div style=" display:flex; justify-content: center; margin: 20px auto 40px; width: 100%;">

  <iframe style="border-radius: 20px;" width="100%" height="300" src="{{ block.settings.video_1 }}" name="featured" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
      


</div>

{% if block.settings.video_1 != blank %}
<div class="embed-responsive embed-responsive-16by9" style=" display:flex; justify-content: center; margin: 20px auto; width: 100%;">
<a class="pr" style="width:100%;" target="featured" href="{{ block.settings.video_1 }}">

  {% if block.settings.video_1 %}
    {% assign youtubeVideoId = block.settings.video_1 | split: 'https://www.youtube.com/embed/' %}
      <div  style="border-radius: 20px; background-image: url('https://img.youtube.com/vi/{{ youtubeVideoId }}/hqdefault.jpg'); height: 200px; width: 100%; background-position:center; background-repeat: no-repeat; background-size: cover;"></div>
  {% endif  %}

<h4 style="margin-top:5px; margin-bottom: 0;">{{ block.settings.video_1_title }}</h4>
</a>

<a class="pl" style="width:100%;" target="featured" href="{{ block.settings.video_2 }}">
  {% if block.settings.video_2 %}
    {% assign youtubeVideoId = block.settings.video_2 | split: 'https://www.youtube.com/embed/' %}
      <div style="border-radius: 20px; background-image: url('https://img.youtube.com/vi/{{ youtubeVideoId }}/hqdefault.jpg'); height: 200px; width: 100%; background-position:center; background-repeat: no-repeat; background-size: cover;"></div>
  {% endif  %}
<h4 style="margin-top:5px;  margin-bottom: 0;">{{ block.settings.video_2_title }}</h4>
</a>
</div>
{% endif %}

<!--      Video Section 2-->


{% if block.settings.video_3 != blank %}
<div class="embed-responsive embed-responsive-16by9" style=" display:flex; justify-content: center; margin: 20px auto; width: 100%;">
<a class="pr" style="width:100%;" target="featured" href="{{ block.settings.video_3 }}">
  {% if block.settings.video_3 %}
    {% assign youtubeVideoId = block.settings.video_3 | split: 'https://www.youtube.com/embed/' %}
